<Window x:Class="SE160143_Ass1.ListBooksForMem"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SE160143_Ass1"
        mc:Ignorable="d"
        Title="ListBooksForMem" Height="450" Width="800">
    <Grid>
        <GroupBox Header="Book List" Margin="10,241,10,10">
            <DataGrid x:Name="dgBook" d:ItemsSource="{d:SampleData ItemCount=5}" Height="150"/>
        </GroupBox>
    </Grid>
</Window>
